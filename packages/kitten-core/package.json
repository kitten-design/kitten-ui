{
  "name": "@kitten-ui/core",
  "version": "0.0.2",
  "type": "module",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "author": "Yandif <me.yandif@gmail.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/kitten-design/kitten-ui.git",
    "type": "git",
    "directory": "packages/kitten-core"
  },
  "keywords": [
    "kitten-ui",
    "react",
    "ui",
    "library",
    "frontend",
    "hooks"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "panda codegen --clean",
    "gen-css": "panda cssgen --outfile dist/styles.css",
