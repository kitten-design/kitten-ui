{
  "name": "@kitten-ui/core",
  "version": "0.0.2",
  "type": "module",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "author": "Yandif <me.yandif@gmail.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/kitten-design/kitten-ui.git",
    "type": "git",
    "directory": "packages/kitten-core"
  },
  "keywords": [
    "kitten-ui",
    "react",
    "ui",
    "library",
    "frontend",
    "hooks",
    "panda",
    "pandacss"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "panda codegen --clean",
    "dev": "vite",
    "build": "rimraf ./lib && rimraf ./dist &&  pnpm build:lib && vite build && pnpm ship",
    "build:lib": "tsc  --project ./tsconfig.build.json",
    "ship": "panda ship --outfile dist/panda.json",
    "studio": "panda studio -c ./panda.config.ts"
  },
  "dependencies": {
    "@kitten-ui/dataset": "workspace:*",
    "@kitten-ui/hooks": "workspace:*",
    "@kitten-ui/styles": "^0.0.3",
    "@kitten-ui/utils": "workspace:*",
    "@tabler/icons-react": "^2.32.0",
    "chroma-js": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.12.0",
    "@pandacss/types": "^0.12.0",
    "@types/chroma-js": "^2.4.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-transition-group": "^4.4.6",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.2"
  }
}
