{
  "name": "@kitten-ui/core",
  "version": "0.0.14",
  "type": "module",
  "main": "dist/index.js",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "sideEffects": false,
  "author": "Yandif <me.yandif@gmail.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/kitten-design/kitten-ui.git",
    "type": "git",
    "directory": "packages/kitten-core"
  },
  "keywords": [
    "kitten-ui",
    "react",
    "ui",
    "library",
    "frontend",
    "hooks",
    "panda",
    "pandacss"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "panda codegen --clean",
    "dev": "vite",
    "build": "rimraf ./lib && rimraf ./dist &&  pnpm build:lib && vite build && pnpm ship",
    "build:lib": "tsc  --project ./tsconfig.build.json",
    "ship": "panda ship --outfile dist/panda.json",
    "studio": "panda studio -c ./panda.config.ts"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.2",
    "@kitten-ui/dataset": "workspace:^",
    "@kitten-ui/hooks": "workspace:^",
    "@kitten-ui/styles": "^0.0.14",
    "@kitten-ui/utils": "workspace:^",
    "@tabler/icons-react": "^2.40.0",
    "chroma-js": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.18.3",
    "@pandacss/types": "^0.18.3",
    "@types/chroma-js": "^2.4.3",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-transition-group": "^4.4.9",
    "@vitejs/plugin-react-swc": "^3.4.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.3"
  }
}
